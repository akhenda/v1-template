{
  "name": "@repo/auth",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./*": {
      "types": "./dist/libs/*/index.d.ts",
      "default": "./libs/*/index.ts"
    },
    "./components": {
      "types": "./dist/libs/*/components/index.d.ts",
      "default": "./libs/*/components/index.tsx"
    },
    "./components/*": {
      "types": "./dist/libs/*/components/*.d.ts",
      "default": "./libs/*/components/*.tsx"
    },
    "./keys": {
      "types": "./dist/keys.d.ts",
      "default": "./keys.ts"
    },
    "./types": {
      "types": "./dist/libs/shared/types.d.ts",
      "default": "./libs/shared/types.ts"
    },
    "./*/provider": {
      "types": "./dist/libs/*/provider.d.ts",
      "default": "./libs/*/provider.tsx"
    },
    "./*/middleware": {
      "types": "./dist/libs/*/middleware.d.ts",
      "default": "./libs/*/middleware.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "typecheck": "tsc --emitDeclarationOnly false"
  },
  "dependencies": {
    "@clerk/chrome-extension": "2.5.13",
    "@clerk/clerk-react": "5.37.0",
    "@clerk/nextjs": "6.27.0",
    "@clerk/themes": "2.4.2",
    "@t3-oss/env-nextjs": "0.13.4",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "server-only": "0.0.1",
    "zod": "3.25.28"
  },
  "devDependencies": {
    "@clerk/types": "4.70.1",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "22.15.21",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "typescript": "5.8.3"
  }
}
