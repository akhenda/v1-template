    "dotenv-expand": "12.0.1",
    "drizzle-orm": "0.41.0",
    "drizzle-zod": "0.6.1",
    "pg": "8.13.1",
    "postgres": "3.4.5",
    "remeda": "2.19.0",
    "server-only": "0.0.1",
    "ulid": "2.3.0",
    "undici": "7.7.0",
