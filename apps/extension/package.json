{
  "name": "extension",
  "displayName": "Kaidoku Cars",
  "version": "0.0.1",
  "description": "Decode Japanese auction sheets â€” bid smarter.",
  "author": "Kaidoku Cars Team",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "clean": "git clean -xdf .cache .turbo dist build .plasmo node_modules",
    "format": "biome format --write .",
    "lint": "biome check ./src",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui --watch",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage --coverage.reporter=text",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "plasmo": "0.90.5",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/chrome": "0.1.1",
    "@types/node": "22.15.21",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "typescript": "5.8.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
}
